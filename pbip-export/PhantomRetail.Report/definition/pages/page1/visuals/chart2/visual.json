{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/report/definition/visualContainer/2.5.0/schema.json",
  "name": "chart2",
  "position": {
    "x": 640,
    "y": 80,
    "z": 5000,
    "width": 640,
    "height": 200,
    "tabOrder": 5
  },
  "visual": {
    "visualType": "pieChart",
    "query": {
      "queryState": {
        "Category": {
          "projections": [
            {
              "field": {
                "Column": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "Product"
                    }
                  },
                  "Property": "category"
                }
              },
              "queryRef": "Product.category",
              "nativeQueryRef": "category"
            }
          ]
        },
        "Y": {
          "projections": [
            {
              "field": {
                "Measure": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "Sale"
                    }
                  },
                  "Property": "Total Revenue"
                }
              },
              "queryRef": "Sale.Total Revenue",
              "nativeQueryRef": "Total Revenue"
            }
          ]
        }
      }
    },
    "objects": {},
    "drillFilterOtherVisuals": true
  }
}
